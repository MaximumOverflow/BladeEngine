<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        SizeToContent="WidthAndHeight" CanResize="False"
        x:Class="BladeEngine.Editor.UI.WarningDialog"
        Title="Warning">
    <StackPanel Margin="16" Spacing="16" HorizontalAlignment="Center" VerticalAlignment="Center">
        <TextBlock Name="Text" Text="Warning message"/>
        <Button HorizontalAlignment="Center" Click="Close">Close</Button>
    </StackPanel>
</Window>
