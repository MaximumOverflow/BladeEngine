<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="600"
        Width="800" Height="600" MinWidth="800" MinHeight="600"
        x:Class="BladeEngine.Editor.UI.BuildProcessWindow"
        Title="BuildProcessWindow">
    <Grid RowDefinitions="Auto, * , Auto">
        <TextBlock Grid.Row="0" Name="Title" FontSize="24" TextAlignment="Center">Building project...</TextBlock>
        <ScrollViewer Grid.Row="1" Name="Scroll">
            <TextBlock Name="Text" FontSize="18"/>
        </ScrollViewer>
    </Grid>
</Window>
